{
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "cleanUrls": true,
  "public": true,
  "images": {
    "domains": ["rohit661x-github-io.vercel.app"],
    "formats": ["image/jpeg", "image/png"],
    "minimumCacheTTL": 60
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 